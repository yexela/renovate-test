{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "gomod"
      ],
      "enabled": true
    }
  ],
  "labels": [
    "dependencies",
    "renovate"
  ],
  "timezone": "UTC",
  "commitMessagePrefix": "chore(deps): ",
  "automerge": false,
  "automergeType": "pr",
  "prCreation": "not-pending",
  "dependencyDashboard": true
}
